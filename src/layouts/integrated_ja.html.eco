  <script type="text/javascript">
    $(function(){
      var opened = false;
      var commentForm = $('.comments');
      $('body').click(function(e) {
        if (opened && !$.contains(commentForm[0], e.target)) {
          commentForm.find('.closed').css('display','block');
          commentForm.find('.opened').css('display','none');
          opened = false;
        }
      });
      $('.btn-open').click(function() {
        var $thisForm = $(this).closest('.comments');
        $thisForm.find('.closed').css('display','none');
        $thisForm.find('.opened').css('display','block');
        $thisForm.find('input[name=name]').focus();
        opened = true;
      });
      $('.btn-send').click(function() {
        var $thisForm = $(this).closest('.comments');
        $thisForm.find('.icon-loading').css('display', 'inline');
        $(this).attr('disabled', 'disabled');
        
        $.ajaxSetup({crossDomain : true});
        var params = {
          message : $("textarea[name=message]", $thisForm).val().replace(/^\s+|\s+$/g, ""),
          email   : $("input[name=email]", $thisForm).val().replace(/^\s+|\s+$/g, ""),
          name    : $("input[name=name]", $thisForm).val().replace(/^\s+|\s+$/g, ""),
          type    : "docs",
          from    : document.domain,
          url     : document.URL
        };
        $.post("https://web.monaca.mobi/api/feedback", params, function() {
          $thisForm.find('.icon-loading').css('display','none');
          $thisForm.find('.btn-send').css('display','none');
          $thisForm.find('.thanks').css('display','block');
        }).error(function() {
                      alert("Failed to send feedback.");
                    $thisForm.find('.icon-loading').css('display','none');
          $thisForm.find('.btn-send').removeAttr('disabled');
        });
        return false;
      });

      $("a.external").each(function() {
          if ($(this).attr("href").indexOf("http") != -1) {
            $(this).attr("target", "_blank").addClass("internet-link");
         }
         });
      $(".section img").each(function() {
          if ($(this).css("width") !== "0px") {
            var parent = $(this).parent().get(0);
            if (parent.tagName.toUpperCase() === "A") {
              if (parent.href.indexOf(".png") > 0 || parent.href.indexOf(".jpg") > 0) {
                parent.className += " fancybox";
              }
            }
          }
          });
      $(".fancybox").fancybox({
        closeBtn: false,
        openEffect: 'elastic',
        modal: false
          
        });
    });
    
    //Create Link object.
    var Link = (function(){

      var url = String(location.href);
      var newUrl = "";
      var detecter = detectLang(url);

      //This function is used to detect language.
      function detectLang(url){
        return "en";
      };
      
      //This function is uded to switch language to another one.
      function switchLang(){ 

          switch(detecter){
              case "en":
                  newUrl = url.replace(/en/,'ja');
              break;

              case "ja":
                  newUrl = url.replace(/ja/,'en');
              break;
          }

          location.href = newUrl;
      };

      //This function is used to open AsialBlog with the specified language.
      function openBlog(){

          switch(detecter){
              case "en":
                  window.open('http://blog.asial.co.jp/?lang=en');
              break;

              case "ja":
                  window.open('http://blog.asial.co.jp/');
              break;        
          }
      };

      function openHp(){

          switch(detecter){
              case "en": 
                  window.open('http://www.asial.co.jp/en/');
              break;

              case "ja":
                  window.open('http://www.asial.co.jp/'); 
              break;        
          }
      }      

      return{
          //return function to trnsit to another language page.
          switchLang : switchLang,
          openBlog : openBlog,
          openHp : openHp
      };

    }()); 

  </script>
